<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
  <settings>
    <setting name="mapUnderscoreToCamelCase" value="true"/>
    <setting name="jdbcTypeForNull" value="NULL" />
    <setting name="logImpl" value="STDOUT_LOGGING"/>
  </settings>

  <typeAliases>
    <typeAlias type="com.example.demo.dto.Pager" alias="Pager"/>
    <typeAlias type="com.example.demo.dto.ChatRoom" alias="ChatRoom"/>
    <typeAlias type="com.example.demo.dto.ChatMessage" alias="ChatMessage"/>
    <typeAlias type="com.example.demo.dto.Comment" alias="Comment"/>
    <typeAlias type="com.example.demo.dto.Pet" alias="Pet"/>
    <typeAlias type="com.example.demo.dto.Post" alias="Post"/>
    <typeAlias type="com.example.demo.dto.Review" alias="Review"/>
    <typeAlias type="com.example.demo.dto.User" alias="User"/>
    <typeAlias type="com.example.demo.dto.Walk" alias="Walk"/>
    <typeAlias type="com.example.demo.dto.Tag" alias="Tag"/>
    <typeAlias type="com.example.demo.dto.PostTag" alias="PostTag"/>
  </typeAliases>
</configuration>
